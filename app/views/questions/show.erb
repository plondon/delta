<div class="container">
  <h1>Questions for <%= @start_language.capitalize %> programmers</h1>
  <% @questions.each do |question| %>
    <% if !question[1].empty? %>
      <div class="end_language">
        <h3><%= question[0] %></h3>
        <ol>
          <% question[1].each do |question| %>
            <li><%= question.query %></li>
          <% end %>
        </ol>
      </div>
    <% else %>
      <%= nil %>
    <% end %>
  <% end %>
</div>
